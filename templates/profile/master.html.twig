<div class="page-content p" style="margin-top: 87px">
    <div class="container">
        <div class="resume-container">
            <div class="shadow-1-strong bg-white my-5" id="intro">
                <div class="bg-info text-white">
                    <div class="cover bg-image"><img src="profil/images/header-background.jpg"/>
                        <div class="mask" style="background-color: rgba(0,0,0,0.7);backdrop-filter: blur(2px);">
                            <div class="text-center p-5">
                                <div class="avatar p-1"><img class="img-thumbnail shadow-2-strong" src="uploads/membres/{{ app.user.image }}" width="160" height="160"/></div>
                                <div class="header-bio mt-3">
                                    <div data-aos="zoom-in" data-aos-delay="0">


                                        <h2 class="h1">{{ app.user.prenom }} {{ app.user.nom }}</h2>
                                        <p>{{ app.user.etablissement }}</p>


                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="shadow-2-strong bg-white my-5 p-5" id="about">
                <div class="about-section p-5">
                    <div class="row">
                        <div class="col-md-6 shadow-1">
                            <h2 class="h2 fw-light mb-4">Introduction</h2>
                            <hr>
                            {{ app.user.introduction }}
                        </div>

                        <div class="col-md-5 col offset-lg-1 shadow-1">
                            <div class="row mt-2">
                                <h2 class="h2 fw-light mb-4 ">Information</h2>
                                <hr>
                                <div class="col-sm-5">
                                    <div class="pb-2 fw-bolder"><i class="far pe-2 text-muted" style="width:24px;opacity:0.85;"></i> Nom</div>
                                </div>
                                <div class="col-sm-7">
                                    <div class="pb-2">        {{ app.user.nom }}</div>
                                </div>
                                <div class="col-sm-5">
                                    <div class="pb-2 fw-bolder"><i class="far  pe-2 text-muted" style="width:24px;opacity:0.85;"></i> Prenom</div>
                                </div>
                                <div class="col-sm-7">
                                    <div class="pb-2">        {{ app.user.prenom }}</div>
                                </div>
                                <div class="col-sm-5">
                                    <div class="pb-2 fw-bolder"><i class="fab pe-2 text-muted" style="width:24px;opacity:0.85;"></i> Specialite</div>
                                </div>
                                <div class="col-sm-7">
                                    <div class="pb-2">        {{ app.user.specialite }}</div>
                                </div>
                                <div class="col-sm-5">
                                    <div class="pb-2 fw-bolder"><i class="fas fa-envelope pe-2 text-muted" style="width:24px;opacity:0.85;"></i> Email</div>
                                </div>
                                <div class="col-sm-7">
                                    <div class="pb-2">       {{ app.user.email }}</div>
                                </div>
                                <div class="col-sm-5">
                                    <div class="pb-2 fw-bolder"><i class="fas fa-phone pe-2 text-muted" style="width:24px;opacity:0.85;"></i> Phone</div>
                                </div>
                                <div class="col-sm-7">
                                    <div class="pb-2">       {{ app.user.phone }}</div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div style="margin-bottom: 32px" id="PR">
            <div class="row" >
                <div class="col-sm-12">
                    <div class="card-body">
                        <ul class="nav nav-tabs">
                            <li>
                                <a class="nav-link active text-uppercase " aria-selected="true">PROJET DE RECHERCHE</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>


            <div class="shadow-1-strong bg-white my-2 p-5">
                <div class="">
                    <div class="row">
                        <div class="col-md">

                            {% for pr in prech %}
                                <div class="timeline">
                                    <div class="timeline-card timeline-card-primary card shadow-sm">
                                        <div class="card-body">
                                            <div class="h5 mb-2">{{ pr.titre }}
                                                <br>
                                                <div class="mt-2">
                                                    <span class="text-muted h6">{{ pr.coordinateurPrinc }} &&</span>
                                                    <span class="text-muted  h6">
                                                       {% for l in pr.Membres %}
                                                           {{ l}} ,
                                                       {% endfor %}
                                                    </span>
                                                </div>
                                            </div>
                                            <div class="text-muted text-small mb-2 "> à {{ pr.dateDebut | date("20y/m/d") }}  jusqu'a {{ pr.datefin | date("20y/m/d") }}   </div>
                                            <div>{{ pr.description}}</div>
                                            <span class="text-muted h6">{{ pr.coordinateurPrinc }} &&</span>

                                            <a   class=" bi-file-pdf" href="{{ asset("uploads/pdf/projetrecherche/")}}{{ pr.pdf}}">
                                                <p class="icon-3d_rotation " >DOWNLOAD</p>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                                <hr>
                            {% endfor %}

                        </div>
                    </div>
                </div>
            </div>
        </div>





        <div style="margin-bottom: 32px" id="SR">
            <div class="row" >
                <div class="col-sm-12">
                    <div class="card-body">
                        <ul class="nav nav-tabs">
                            <li>
                                <a class="nav-link active text-uppercase " aria-selected="true">SUJET DE RECHERCHE</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>


            <div class="shadow-1-strong bg-white my-2 p-5">
                <div class="">
                    <div class="row">
                        <div class="col-md">

                            {% for sr in sujetR%}
                                <div class="timeline">
                                    <div class="timeline-card timeline-card-primary card shadow-sm">
                                        <div class="card-body">
                                            <div class="h5 mb-2">{{ sr.titre }}
                                                <br>
                                                <div class="mt-2">
                                                    <span class="text-muted h6">{{ sr.encadren }} &&</span>
                                                    <span class="text-muted  h6"> {{ sr.master }} {{ sr.stagiaire }}</span>
                                                </div>
                                            </div>
                                            <div class="text-muted text-small mb-2 "> à {{ sr.datedebut | date("20y/m/d") }}  jusqu'a {{ sr.datefine | date("20y/m/d") }}   </div>
                                            <div>{{ sr.description}}</div>
                                            <span class="text-muted h6">associe à {{ sr.prAssocie }}</span>
                                            <span class="text-muted h6">{{ sr.pfe }}</span>
                                            <br>
                                            <a   class=" bi-file-pdf" href="{{ asset("uploads/pdf/sujetrecherche/")}}{{ sr.pdf}}">
                                                <p class="icon-3d_rotation " >DOWNLOAD</p>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                                <hr>
                            {% endfor %}

                        </div>
                    </div>
                </div>
            </div>
        </div>
        <script src="{{ asset('assets2/vendor/aos/aos.js') }}"></script>
        <script src="assets2/vendor/glightbox/js/glightbox.min.js"></script>
        <script src="assets2/vendor/swiper/swiper-bundle.min.js"></script>
        <!-- Template Main JS File -->
        <script src="assets2/js/main.js"></script>
        <script src="profil/scripts/aos.js?ver=1.2.1"></script>
        <script src="profil/scripts/main.js?ver=1.2.1"></script>
    </div>
</div>
