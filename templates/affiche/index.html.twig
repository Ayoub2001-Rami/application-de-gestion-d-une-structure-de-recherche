</header><!-- End Header -->

<!-- ======= Frequently Asked Questions Section ======= -->
<section id="faq" class="faq section-bg">
    <div class="container" data-aos="fade-up">

        <div class="section-title">
            <h2>VOTRE SUJET DE THESE</h2>
        </div>

        {% for s in sujet %}
            <div class="faq-list">
                <ul>
                    <li data-aos="" data-aos-delay="100">
                        <a data-bs-toggle="collapse" class="collapse" data-bs-target="">_Titre  :{{ s.titre }}</a>
                        <a data-bs-toggle="collapse" class="collapse" data-bs-target="">_Coencadrant  :{{ s.coencadrant }}</a>
                        <a data-bs-toggle="collapse" class="collapse" data-bs-target="">_Encadrant  :{{ s.encadrant }}</a>
                        <a data-bs-toggle="collapse" class="collapse" data-bs-target=""> _Bourse  :{{ s.bourse }}</i></a>
                        <a data-bs-toggle="collapse" class="collapse" data-bs-target="">_Annee :{{ s.annee|date('Y/M/D')}}</a>
                        <div id="faq-list-1" class="collapse show" data-bs-parent=".faq-list">
                            <p>
{#<i class="bx bx-chevron-down icon-show"></i><i class="bx bx-chevron-up icon-close"></i> #}
                            <hr>
                            {{ s.description }}
                         </p><hr>
                            <div class="bi-file-earmark-pdf">{{ s.pdf }}</div>
                        </div>
                    </li>

                </ul>
            </div>
        {% endfor %}

    </div>
</section><!-- End Frequently Asked Questions Section -->
